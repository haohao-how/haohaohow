{
  "buildCommand": "",
  "outputDirectory": "dist/vercel/client",
  "devCommand": "",
  "functions": {
    "api/index.cjs": {
      "maxDuration": 25,
      "runtime": "@vercel/node@5.1.13",
      "includeFiles": "dist/vercel/server/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.cjs"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31556952, immutable"
        }
      ]
    }
  ]
}
